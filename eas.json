{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "production": {

      "autoIncrement": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL":"https://recipes-api-prod.onrender.com/"
      },
      "channel": "production"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL":"https://recipes-api-test-600h.onrender.com/"
      },
      "channel": "development"
    }
  }
}
